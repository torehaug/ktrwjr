<?xml version="1.0" encoding="UTF-8"?>
<module
  rename-to='ktrwjr'>
  <!-- gwt core -->
  <inherits
    name='com.google.gwt.user.User' />
  <!-- http & json -->
  <inherits
    name='com.google.gwt.http.HTTP' />
  <inherits
    name='com.google.gwt.json.JSON' />


  <!-- entry point -->
  <entry-point
    class='bufferings.ktr.wjr.client.KtrWjr' />

  <!-- source -->
  <source
    path="client" />
  <source
    path="shared" />
</module>
