<?xml version="1.0" encoding="UTF-8"?>
<web-app
  xmlns="http://java.sun.com/xml/ns/javaee"
  version="2.5">

  <servlet>
    <servlet-name>KtrWjrJsonServiceServlet</servlet-name>
    <servlet-class>bufferings.ktr.wjr.server.service.KtrWjrJsonServiceServlet
    </servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>KtrWjrJsonServiceServlet</servlet-name>
    <url-pattern>ktrwjr/ktrwjr.json</url-pattern>
  </servlet-mapping>

  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
</web-app>
